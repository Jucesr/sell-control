{
  "name": "sell-control",
  "version": "1.0.0",
  "description": "A profesional application that is suitable for any small-medium retail store. It allows to take control of clients, products, suppliers and sellings.",
  "main": "index.js",
  "scripts": {
    "scss-compile": "node-sass --watch src/styles/master.scss -o public/dist",
    "live-server": "live-server public | npm run scss-compile",
    "babel-compile": "npx babel src/app.jsx --watch --out-file public/dist/bundle.js",
    "dev-server": "webpack-dev-server --config webpack.dev.js",
    "build-dev": "webpack --display-modules --sort-modules-by size",
    "build-prd": "webpack -p --config webpack.prd.js --display-modules --sort-modules-by size",
    "start": "node server/server.js",
    "start-babel": "babel-node server/server.js",
    "start-moon": "nodemon --watch server server/server.js --exec babel-node",
    "start-test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && babel-node server/server.js",
    "heroku-postbuild": "npm run build-prd ",
    "test": "export LOG=true || SET \"LOG=true\" && jest",
    "test-server": "export LOG=true && export NODE_ENV=jest || SET \"NODE_ENV=jest\" && SET \"LOG=true\" && jest --watch --runInBand",
    "test-debugger": "export LOG=true && export NODE_ENV=jest || SET \"NODE_ENV=jest\" && SET \"LOG=true\" && node inspect ./node_modules/jest/bin/jest.js server\/test\/(?!i) --runInBand --watch"
  },
  "keywords": [
    "sell",
    "controll",
    "react",
    "redux",
    "javascript",
    "crud",
    "punto",
    "de",
    "venta"
  ],
  "author": "Julio Ojeda",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "jest": "^22.4.3",
    "live-server": "^1.2.0",
    "node-sass": "^4.7.2",
    "preload-webpack-plugin": "^2.3.0",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "supertest": "^3.0.0",
    "webpack": "^3.1.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cleave.js": "^1.3.3",
    "cross-fetch": "^2.1.0",
    "express": "^4.16.3",
    "formik": "^0.11.11",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "moment": "^2.21.0",
    "mongodb": "^3.0.4",
    "mongoose": "^5.0.11",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-modal": "^3.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-table": "^6.8.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "validator": "^9.4.1",
    "yup": "^0.24.1"
  }
}
